# Change DIR to the directory that contains the SpecTcl source files
DIR=../SpecTcl

# Change WISH to the pathname of your tk4.1a2 (or >) wish binary
WISH=`which wish`

env SPECTCL_DIR=$DIR $WISH $DIR/main.tk -java $@ &
echo "Starting SpecTcl 1.0"
exit 0
