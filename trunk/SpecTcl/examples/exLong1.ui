# Bind Return-key event for entry widgets to proc
bind .name <Key-Return> {ShowEntry %W}
bind .company <Key-Return> {ShowEntry %W}
bind .e_mail <Key-Return> {ShowEntry %W}

proc ShowEntry {w} {
  puts "You entered [$w get] in $w"
}


proc ShowButton {w} {
  puts "You pushed $w"
}



















